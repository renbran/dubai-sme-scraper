{
  "name": "dubai-sme-scraper",
  "version": "1.0.0",
  "description": "Professional-grade Apify actor for scraping Small and Medium Enterprises (SMEs) in Dubai from Google Maps with automatic Google Sheets integration",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "build": "apify build",
    "push": "apify push",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "test:local": "jest --testPathPattern=local",
    "test:sheets": "jest --testPathPattern=sheets",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "dev": "nodemon src/main.js"
  },
  "keywords": [
    "apify",
    "scraper",
    "google-maps",
    "dubai",
    "sme",
    "business-data",
    "google-sheets",
    "playwright",
    "data-extraction"
  ],
  "author": "Apify Actor Developer",
  "license": "MIT",
  "dependencies": {
    "apify": "^3.1.0",
    "cheerio": "^1.0.0-rc.12",
    "csv-writer": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "playwright": "^1.40.0",
    "puppeteer": "^24.20.0",
    "retry": "^0.13.1",
    "uuid": "^9.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@apify/eslint-config": "^0.4.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/renbran/dubai-sme-scraper.git"
  }
}
